{
  "name": "@utima/eslint-config",
  "version": "0.19.0",
  "description": "Utima's extensible shared eslint-config. It contains opinionated rules for JavaScript and TypeScript projects including React, Next.JS, jest and more.",
  "main": "./index.js",
  "license": "MIT",
  "homepage": "https://github.com/utima-solutions/eslint-config",
  "bugs": {
    "url": "https://github.com/utima-solutions/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/utima-solutions/eslint-config"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./dist/cjs/index.js"
    },
    "./configs": {
      "import": "./src/configs/index.js",
      "require": "./dist/cjs/src/configs/index.js",
      "types": "./src/configs/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint './**/*.{js,ts,jsx,tsx,cjs,mjs}'",
    "changeset": "changeset add",
    "release": "utima release -d ''"
  },
  "dependencies": {
    "@changesets/cli": "^2.29.5",
    "@next/eslint-plugin-next": "^14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import-x": "^3.1.0",
    "eslint-plugin-jest": "^28.8.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-sonarjs": "^1.0.4",
    "eslint-plugin-sort-class-members": "^1.21.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.9.0",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.38.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@utima/scripts": "^0.1.3",
    "eslint": "^8.57.0",
    "rollup": "^4.46.2"
  },
  "peerDependencies": {
    "eslint": "8"
  }
}
